// server.js (lihtne test, mitte tootmises kasutamiseks)
const express = require('express');
const app = express();
const PORT = process.env.PORT || 4000;

app.get('/', (req, res) => {
  res.send('Test server töötab! Aga Next.js käivita npm start kaudu.');
});

app.listen(PORT, () => {
  console.log(`Express server: http://localhost:${PORT}`);
});